#include <iostream>

using namespace std;

int main()
{
    int N;
    std::cout<<"Kolvo: "<<std::endl;
    std::cin>>N;
    N=N+1;
    for(int i=1; i<N; i++) {
        for (int j=0; j<i; j++)
            std::cout<<0;
        std::cout<<std::endl;
    }
    std::cout<<std::endl;

    std::getchar();
    std::getchar();
    return 0;
}
